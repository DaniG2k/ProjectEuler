#!/usr/bin/python
import time
start = time.time()
def triangle(n): return n*(n+1)/2
def pentagonal(n): return n*(3*n-1)/2
def hexagonal(n): return n*(2*n-1)

dic = {}
for i in xrange(144,56000):
    tri = triangle(i)
    pen = pentagonal(i)
    hex = hexagonal(i)
    if tri not in dic: dic[tri] = 1
    elif tri in dic: dic[tri] += 1
    if pen not in dic: dic[pen] = 1
    elif pen in dic: dic[pen] += 1
    if hex not in dic: dic[hex] = 1
    elif hex in dic: dic[hex] += 1

    if dic[tri] == 3:
        print tri
        break
end = time.time()
print end-start
